
# <Multi_key> <g> <a> : "α"
#
# See here for more examples: https://github.com/kragen/xcompose/blob/master/dotXCompose

<Multi_key> <p> <r> <e> : "≺"  # precedence; "depends on"
<Multi_key> <s> <u> <c> : "≻"  # succesion
<Multi_key> <less> <greater> : "⇔"  # iff
<Multi_key> <bar> <underscore> : "⟂"  # independence, orthogonal
<Multi_key> <bar> <bar> <underscore> : "⫫"  # conditional independence
<Multi_key> <greater> <greater> : "→"  # this overrides "»"
<Multi_key> <less> <less> : "←"
<Multi_key> <asterisk> <o> : "ω"
<Multi_key> <asterisk> <O> : "Ω"
# u is theta (h is eta, e is epsilon, t is tau)
<Multi_key> <minus> <less> <greater> : ""  # this overrides vanilla "<-" i.e. ←
